{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/config/(.*)": "<rootDir>/../libs/config/src/$1",
    "@app/config": "<rootDir>/../libs/config/src",
    "@app/mongo/(.*)": "<rootDir>/../libs/mongo/src/$1",
    "@app/mongo": "<rootDir>/../libs/mongo/src",
    "@app/push/(.*)": "<rootDir>/../libs/push/src/$1",
    "@app/push": "<rootDir>/../libs/push/src",
    "@app/authorization-code/(.*)": "<rootDir>/../libs/authorization-code/src/$1",
    "@app/authorization-code": "<rootDir>/../libs/authorization-code/src"
  }
}